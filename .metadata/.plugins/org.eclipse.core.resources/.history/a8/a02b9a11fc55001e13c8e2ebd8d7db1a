package term1;

public class Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=-5, b=10, c=100;
		//1
		System.out.println(a==-5);
		//2
		System.out.println(a!=-5);
		//3
		System.out.println(b<10);
		//4
		System.out.println(b<=10);
		//5
		System.out.println(c>=50);
		//6
		System.out.println(c++>=101);
		

	}

}
