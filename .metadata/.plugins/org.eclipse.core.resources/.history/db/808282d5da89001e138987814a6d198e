package examT2;

import java.util.Scanner;

public class Pregunta1 {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int num;
		do {
			System.out.print("the code of the province (03,12 o 46): ");
			num = sc.nextInt();
			
			if (num!=03 || num!=12 || num !=46){
				System.out.println("whrite a correct code");
			}
			
		} while (num ==03 || num==12 || num==46);
		System.out.println("correct code");
	}

}
